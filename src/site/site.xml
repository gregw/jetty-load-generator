<?xml version="1.0" encoding="UTF-8"?>
<project name="Reflow Maven Skin"
         xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

  <bannerLeft>
    <name>Webtide</name>
    <src>webtide-logo.png</src>
    <href>https://www.webtide.com</href>
  </bannerLeft>

  <bannerRight>
    <src>jetty-logo-shadow-400x114.png
    </src>
    <href>http://www.eclipse.org/jetty/</href>
  </bannerRight>


  <publishDate position="bottom" format="yyyy-MM-dd" />
  <version position="bottom" />

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.6</version>
  </skin>

  <custom>
    <fluidoSkin>
      <topBarEnabled>false</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <gitHub>
        <projectId>jetty-project/jetty-load-generator</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>black</ribbonColor>
      </gitHub>
    </fluidoSkin>
  </custom>

  <body>

    <menu name="Main" inherit="top">
      <item name="Home" href="./index.html" />
    </menu>
    <menu name="Download" inherit="top">
      <item name="License" href="license.html" />
    </menu>
    <menu name="Maven documentation" ref="reports" inherit="bottom"/>
  </body>
</project>